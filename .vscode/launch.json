  "version": "0.2.0",
  "configurations": [
    {
      "name": "API",
      "type": "node",
      "request": "launch",
      "args": [
        "${workspaceFolder}/src/api/index.ts"
      ],
      "env": {
        "TWILIO_ACCOUNT_SID": "",
        "TWILIO_AUTH_TOKEN": ""
      },
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register"
      ],
      "sourceMaps": true,
      "cwd": "${workspaceRoot}",
      "protocol": "inspector",
    }
  ]
}